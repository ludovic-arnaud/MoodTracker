<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.arnaud.ludovic.moodtracker.controller.MainActivity">

    <ImageButton
        android:id="@+id/activity_main_comment_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_note_add_black"
        android:layout_gravity="bottom|start"
        android:padding="20dp"
        />

    <ImageButton
        android:id="@+id/activity_main_history_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_history_black"
        android:layout_gravity="bottom|end"
        android:padding="20dp"
        />

</FrameLayout>